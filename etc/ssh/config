# Mixpanel

Host cpp
    User ubuntu
    IdentityFile "~/Google Drive/certs/keys/cpp_workspace_3.pem"
    Hostname ec2-54-193-116-230.us-west-1.compute.amazonaws.com

Host tools
    User ubuntu
    Hostname 52.34.199.252
    IdentityFile ~/.ssh/tools.pem

Host rohith-gcp
    User rohith
    IdentityFile ~/.ssh/id_rsa
    Hostname 35.185.219.216

Host rohith
    User rohith
    IdentityFile ~/.ssh/id_rsa
    Hostname 198.11.223.244
    #ControlPath ~/.ssh/cm-%r@%h:%p
    #ControlMaster auto

Host rravi
  User rravi
  ForwardAgent yes
  ProxyJump  -A -J ssh-bastion.gcp.corp.mixpanel.org %h

Host bastion1
    User rohith
    Hostname ssh-bastion.gcp.corp.mixpanel.org

Host *.gcp
    User rohith
    ProxyCommand ssh -A %r@bastion1 -W $(h=%h; echo ${h%%.gcp}):%p

Host *.corp.mixpanel.com

Host *arb* api* app* aliases* backup* build* cache* consumer* *cron* *engage* export* integration* liveview* notifications* redis* trends* switchboard* ephemeral-*
    User live

Host *
    ServerAliveInterval 30
    ServerAliveCountMax 3
    GSSAPIAuthentication no
    StrictHostKeyChecking no
    HashKnownHosts no
    ForwardAgent yes
