# Mixpanel

Host rravi-1
    User rravi-1
    ForwardAgent yes
    ProxyCommand ssh -A rohith@ssh-bastion-west.gcp.corp.mixpanel.org -W %h:%p

Host rravi-2
    User rravi-2
    ForwardAgent yes
    ProxyCommand ssh -A rohith@ssh-bastion-west.gcp.corp.mixpanel.org -W %h:%p

Host rravi-2-2fa
    User rravi-2
    Hostname rravi-2
    ForwardAgent yes
    ProxyCommand ssh -A -p 1883 rohith@ssh-bastion-global.mixpanel.org -W %h:%p

Host *
    ServerAliveInterval 30
    ServerAliveCountMax 3
    GSSAPIAuthentication no
    StrictHostKeyChecking no
    HashKnownHosts no
    ForwardAgent yes
