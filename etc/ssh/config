# Mixpanel

Host bastion
    ForwardAgent yes
    User rohithravi
    HostName west-dev-bastion.mixpanel.org
    ServerAliveInterval 5
    ServerAliveCountMax 2
    ControlPath ~/.ssh/controlmasters/%r@%h:%p
    ControlMaster auto
    ControlPersist 10m

Host rravi-2
    User rravi-2
    Hostname rravi-2
    ForwardAgent yes
    ProxyCommand ssh -A bastion -W %h:%p

Host rravi-2-2fa
    User rravi-2
    Hostname rravi-2
    ForwardAgent yes
    ProxyCommand ssh -A -p 1883 bastion -W %h:%p

Host rravi-1
    User rravi
    Hostname devbox-956
    ForwardAgent yes
    ProxyCommand ssh -A bastion -W %h:%p

Host rravi-1-2fa
    User rravi
    Hostname devbox-956
    ForwardAgent yes
    ProxyCommand ssh -A -p 1883 bastion -W %h:%p

Host *
    ServerAliveInterval 30
    ServerAliveCountMax 3
    GSSAPIAuthentication no
    StrictHostKeyChecking no
    HashKnownHosts no
    ForwardAgent yes
